{
    "manifest_version": 2,

    "name": "Tabnarok",
    "description": "A tab stats analyzer.",
    "version": "0.1",

    "permissions": [
        "tabs"
    ],

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
